<Application
    x:Class="Lookup.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:data="clr-namespace:Lookup.ViewModel"
    xmlns:local="clr-namespace:Lookup"
    StartupUri="/Lookup;component/View/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <data:ViewModel x:Key="data" />
            <local:ViewModelLocator x:Key="Lookup.ViewModel.ViewModelLocator" />
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Lookup;component/View/Resources.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>

